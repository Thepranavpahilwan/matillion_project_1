					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"variableName": "var_files",
					"value": {
						"value": "@activity('Get Metadata1').output.childItems",
						"type": "Expression"
					}
				}
			}
		],
		"variables": {
			"var_files": {
				"type": "Array"
			},
			"var3": {
				"type": "String"
			},
			"var4": {
				"type": "String"
			},
			"json_time": {
				"type": "String"
			},
			"order": {
				"type": "String"
			},
			"destination_last_modified": {
				"type": "String"
			},
			"latestTimestamp": {
				"type": "String",
				"defaultValue": "2025-04-10T10:00:00Z"
			},
			"last_modified": {
				"type": "String"
			}
		},
		"folder": {
			"name": "practice"
		},
		"annotations": []
	}
